<div class="main">
  <div class="container">
    <div class="search">
    <h1>Results</h1>
      <div id="result">
          <%@result.each do |user|%>
            <%=image_tag user.profile_picture.representation(resize_to_limit: [250, 250]),id: "searchResult"%>
            <%=link_to "#{user.user_name}",profile_path(user),class: "btn btn primary"%>
          <%end%>
      </div>
  </div>
  </div>
</div>
<style>
    .main
    {
      margin-top: 40px;
    }
    #result
    {
      margin-bottom: 10px;
    }
</style>
