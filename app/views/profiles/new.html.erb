<div class="main">
    <div class="container">
        <h1>Create New Profile</h1>
            <div id="createProfile">
                <%=form_with model: @profile do |form| %>
                <%=form.label :user_name%>
                <%=form.text_field :user_name%>
                <%=form.label :profile_picture%>
                <%=form.file_field :profile_picture%>
                <%=form.submit%>
                 <%end%>
            </div>
    </div>
</div>
